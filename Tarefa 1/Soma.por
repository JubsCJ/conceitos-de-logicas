programa {
  funcao inicio() {
    real numero1, numero2, soma

    escreva ("Digite 1° Numero: ")
    leia (numero1)

    escreva ("Digite 2° Numero: ")
    leia (numero2)

    soma = (numero1 + numero2)
    escreva ("A soma entre ",numero1, " e ",numero2, " é igual a ",soma, " ")
    leia (soma)
  }
}